{
  "name": "storefront-project",
  "version": "1.0.0",
  "description": "This is a REST API simulating the cart functionalities designed with models and handlers tied with endpoints for specific behaviours. A detailed list of actions, schema and endpoint is available in REQUIREMENTS.md.",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/server.ts",
    "test": "cross-env ENV=test npm run migrate-plus-jasmine",
    "start:dist": "tsc && node dist/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jasmine": "jasmine --config=./spec/support/jasmine.json",
    "migrate-plus-jasmine": "db-migrate up -e test && npm run build && npm run jasmine && db-migrate reset -e test"
  },
  "author": "elvis",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.10.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/nodemon": "^1.19.2",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  }
}
